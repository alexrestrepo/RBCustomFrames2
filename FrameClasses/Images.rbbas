#tag ModuleProtected Module Images	#tag Method, Flags = &h0		Function BlurMask() As Picture		  if blurMaskImage = nil then		    blurMaskImage = ImageForData(BlurData)		  end if		  		  Return blurMaskImage		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function CBBackground() As Picture		  if CBImage = nil then		    CBImage = ImageForData(CBData)		  end if		  		  Return CBImage		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function Check() As Picture		  if checkImage = nil then		    checkImage = LoadMaskedImage(ImageForData(CheckData))		  end if		  		  Return checkImage		End Function	#tag EndMethod	#tag Method, Flags = &h21		Private Function ImageForData(data as string) As picture		  dim tmp as new DatabaseRecord		  tmp.Column("dummy") = DecodeBase64(data)		  Return tmp.JPEGColumn("dummy")		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function LoadMaskedImage(Masked as Picture) As picture		  // Just a small convenience method. Not required.		  		  dim pic as Picture		  		  pic = New Picture(Masked.Width / 2, Masked.Height, 32)		  pic.Graphics.DrawPicture Masked, 0, 0, pic.Width, pic.Height, 0, 0, Masked.Width / 2, Masked.Height		  pic.Mask.Graphics.DrawPicture Masked, 0, 0, pic.Width, pic.Height, Masked.Width / 2, 0, Masked.Width / 2, Masked.Height		  		  return pic		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function PopupKnob() As picture		  if PopupKnobImage = nil then		    PopupKnobImage = LoadMaskedImage(ImageForData(KnobData))		  end if		  		  Return PopupKnobImage		End Function	#tag EndMethod	#tag Property, Flags = &h21		Private blurMaskImage As picture	#tag EndProperty	#tag Property, Flags = &h21		Private CBImage As picture	#tag EndProperty	#tag Property, Flags = &h21		Private checkImage As picture	#tag EndProperty	#tag Property, Flags = &h21		Private PopupKnobImage As picture	#tag EndProperty	#tag Constant, Name = BlurData, Type = String, Dy